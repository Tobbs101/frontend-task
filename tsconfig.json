{
  "compilerOptions": {
    "isolatedModules": true,
    "esModuleInterop": true,
    "target": "ES2020",
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["node", "jest", "@testing-library/jest-dom","cypress"]
  },
  "include": ["src","**/*.ts"]
}
